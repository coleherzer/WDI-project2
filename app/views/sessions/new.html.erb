<h1>Sign In</h1>
<%= form_tag sessions_path do |f| %>
    <br /> 
    <%= label_tag :email, 'Email' %>
    <br /> 
    <%= text_field_tag :email, params[:email] %>
    <br /> 
    <%= label_tag :password, 'Password' %>
    <br /> 
    <%= password_field_tag :password %>
    <br /> 
    <%= submit_tag 'Log in' %>
    <br /> 
    <%= link_to "Don't have an account?", new_user_path %>
<% end %>
